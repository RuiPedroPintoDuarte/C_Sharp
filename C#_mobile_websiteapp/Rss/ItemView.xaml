<Window x:Class="Rss.ItemView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Rss"
        mc:Ignorable="d"
        Title="ItemView" Height="450" Width="800">
    <Grid>
        <WebBrowser x:Name="webBrowser" Width="auto" Height="auto" Margin="0,20,0,0"/>
        <Menu Height="20" VerticalAlignment="Top">
            <MenuItem Header="Abrir em browser" Click="MenuItem_Browser"/>
            <MenuItem Header="Arquivar" Click="MenuItem_Arquivar"/>
            <MenuItem Header="Remover" Click="MenuItem_Remover"/>
        </Menu>
    </Grid>

</Window>
